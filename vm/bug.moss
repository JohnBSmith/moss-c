

sub Abb(A,B)
  A = list(A); B = list(B)
  M = B^size(A)
  return M.map(|t| (0..size(t)-1).dict(|i| [A[i],t[i]]))
end

extend(dict.type,table
  sub POW(B)
    A = list(self); B = list(B)
    M = B^size(A)
    return M.map(|t| (0..size(t)-1).dict(|i| [A[i],t[i]]))
  end
end)

