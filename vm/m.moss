

sub Abb(A,B)
  A = list(A); B = list(B)
  M = B^size(A)
  return M.map(|t| (0..size(t)-1).dict(|i| [A[i],t[i]]))
end

extend(dict.type,table
  sub POW(n)
    if type(n) is int.type
      return set(list(self)^n)
    else
      a = list(n)
      c = list(self)^size(a)
      return set(c.map(|t| (0..size(t)-1).dict(|i| [a[i],t[i]])))
    end
  end
end)

