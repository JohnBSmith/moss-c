
sub digamma(x)
  if re(x)<0
    return digamma(1-x)-pi/tan(pi*x)
  elif re(x)<10
    return digamma(x+1)-1/x
  else
    return ln(x)-1/(2*x)-(1..7).sum(sub|n|
      B(2*n)/(2*n*x^(2*n))
    end)
  end
end

gc = 0.57721566490153286
sub polygamma(s,z)
  y = diff(|s| exp(gc*s)*zeta(s+1,z)/gamma(-s),s)
  return exp(-gc*s)*y
end


